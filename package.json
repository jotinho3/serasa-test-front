{
  "name": "personal-blog-app",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:docker": "node --openssl-legacy-provider ./node_modules/@angular/cli/bin/ng serve --host 0.0.0.0",
    "build": "ng build --configuration production",
    "build:docker": "node --openssl-legacy-provider ./node_modules/@angular/cli/bin/ng build --configuration production",
    "test": "jest",
    "docker:build": "docker-compose build",
    "docker:build:dev": "docker-compose -f docker-compose.dev.yml build",
    "docker:up": "docker-compose up",
    "docker:up:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:down": "docker-compose down",
    "docker:down:dev": "docker-compose -f docker-compose.dev.yml down"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~12.0.0",
    "@angular/core": "~12.0.0",
    "@angular/forms": "~12.0.0",
    "@angular/platform-browser": "~12.0.0",
    "@angular/platform-browser-dynamic": "~12.0.0",
    "@angular/router": "~12.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular/cli": "~12.0.0",
    "@angular/compiler": "~12.0.0",
    "@angular/compiler-cli": "~12.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^12.11.1",
    "jest": "^27.5.1",
    "jest-preset-angular": "^10.1.0",
    "typescript": "~4.2.0"
  }
}
